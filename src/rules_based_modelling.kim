		ContinentalSeasonColdSummer 			if 27,
		ContinentalSeasonVeryColdWinter 		if 28,
		PolarTundra 							if 29,
        PolarFrost								if 30;

number local:srwohl:im.data.global:im-data-global-soil.isric_texmht_m_sl1_250m_ll as soil_text;
	
