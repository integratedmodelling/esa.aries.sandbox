 landcover:Grassland	      | #                                          | #									          | #   				|  nca.ecosystem.iucn:DerivedSeminaturalPastureOldfield        ,
 landcover:Shrubland	      | #                                          | nca.ecoregion:SucculentKarooXericShrubland   | #   				|  nca.ecosystem.iucn:TropicalSubtropicalDryForestThicket      , 
